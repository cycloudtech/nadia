    <sparql:query rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SELECT ?lex
WHERE {
?w :has_lex ?lex.
?w :has_context ?con.
{{?con :has_dimension :temporal.}
UNION
{?con :has_dimension :date}}
?w :has_pos :whadv.
FILTER(lang(?lex)="en").
}</sparql:query>
